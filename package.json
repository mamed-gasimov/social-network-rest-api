    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-validator": "^6.12.2",
    "multer": "^1.4.3",
    "mysql2": "^2.3.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^7.0.0-rc.8",
